{
  "name": "nexml-playground-nodejs",
  "version": "0.0.1",
  "description": "The RESTful API server for the Nexml Playground platform. Provides JSON data for the Playground client and communicates with the MCP server.",
  "keywords": [],
  "license": "ISC",
  "type": "commonjs",
  "author": {
    "name": "Andrei Boghiu",
    "url": "https://github.com/Andrei-Boghiu"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/Andrei-Boghiu/nexml-playground-express-service-api"
  },
  "main": "build/index.js",
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node build/index.js",
    "prisma:dev": "npx prisma migrate reset --force && npx prisma migrate dev --name auto && npx prisma generate",
    "prisma:prod": "npx prisma generate && npx prisma migrate deploy"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.884.0",
    "@prisma/client": "^6.15.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.16.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
